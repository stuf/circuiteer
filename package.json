{
  "name": "@etm12/circuiteer",
  "description": "Plan your Astroneer bases to get a shopping list and an overview of power usage",
  "keywords": [
    "partial lenses",
    "astroneer",
    "planner"
  ],
  "author": {
    "email": "stefan@rimaila.fi",
    "name": "Stefan Rimaila",
    "url": "https://github.com/stuf"
  },
  "contributors": [],
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "scripts/generate-env.js && BROWSER=none craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "test:cov": "yarn test --coverage --watchAll=false",
    "test:e2e": "cypress open"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/(canvas|state|_)/index.js",
      "<rootDir>/src/components/\\w+/index.js",
      "typedefs?.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.1.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@visx/annotation": "^1.9.0",
    "@visx/axis": "^1.10.0",
    "@visx/curve": "^1.7.0",
    "@visx/drag": "^1.7.4",
    "@visx/event": "^1.7.0",
    "@visx/glyph": "^1.7.0",
    "@visx/grid": "^1.9.1",
    "@visx/group": "^1.7.0",
    "@visx/pattern": "^1.7.0",
    "@visx/point": "^1.7.0",
    "@visx/responsive": "^1.7.0",
    "@visx/scale": "^1.7.0",
    "@visx/shape": "^1.8.0",
    "@visx/text": "^1.7.0",
    "@visx/tooltip": "^1.7.2",
    "classnames": "^2.3.1",
    "d3-scale-chromatic": "^2.0.0",
    "i18next": "^20.2.2",
    "immer": "^9.0.2",
    "infestines": "^0.4.11",
    "jest-extended": "^0.11.5",
    "kefir": "^3.8.8",
    "linear-algebra": "^3.1.4",
    "partial.lenses": "^14.17.0",
    "partial.lenses.validation": "^2.0.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.3",
    "react-fast-compare": "^3.2.0",
    "react-i18next": "^11.8.15",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.0",
    "ts-toolbelt": "^9.6.0",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/kefir": "^3.8.5",
    "@types/ramda": "^0.27.40",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^9.8.6",
    "cypress": "^7.2.0",
    "dependency-cruiser": "^10.0.0",
    "eslint-plugin-cypress": "^2.11.3",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "tailwindcss-interaction-variants": "^5.0.0",
    "typescript": "^4.2.4"
  }
}
